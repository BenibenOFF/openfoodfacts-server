    "file-loader": "^1.1.11",
    "foundation-icon-fonts": "^0.1.1",
    "foundation-sites": "5.5.3",
    "image-webpack-loader": "^4.2.0",
    "jquery": "^3.3.1",
    "jquery-tags-input": "^1.3.5",
    "jquery-ui": "^1.12.1",
    "leaflet": "~1.0.3",
    "leaflet.markercluster": "^1.3.0",
